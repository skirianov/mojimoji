{
  "name": "MojiMoji",
  "description": "Automatic emoji suggestions!üëç ",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"

  },
  "host_permissions": ["https://*/*"],
    "action": {
      "default_icon": {
        "16": "/MojiMoji.png",
        "32": "/MojiMoji.png",
        "48": "/MojiMoji.png",
        "128": "/MojiMoji.png"
      }
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "webNavigation"
  ],
  "icons": {
    "16": "/MojiMoji.png",
    "32": "/MojiMoji.png",
    "48": "/MojiMoji.png",
    "128": "/MojiMoji.png"
  }
}